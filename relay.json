{
  "repos": [
    {
      "name": "routing",
      "url": "https://github.com/johntdyer/git2consul-testing.git",
      "hooks": [
        {
          "type": "polling",
          "interval": "30s"
        },
        {
          "type": "local"
        }
      ],
      "branches": [ "master", "dev" ],
      "tags": ["v0.0.1"],
      "stores": [ "consul" ]
    }
  ],
  "consul": {
    "client_config": {
      "address": "127.0.0.1:8500"
    },
    "ref_cache": "relay-cache"
  }
}
